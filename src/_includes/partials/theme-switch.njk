<div class="theme-switch" x-data="themeSwitch()" x-init="init">
	<button class="theme-switch-button" id="theme-switch-button" aria-label="Theme Switcher" role="presentation" aria-hidden="true" @click="changeOnClick">
		<svg class="svg-icon icon-inline" shape-rendering="geometricPrecision" role="presentation" aria-label="icon" aria-hidden="true">
			<use xlink: :href="iconStyle"></use>
		</svg>
	</button>
</div>